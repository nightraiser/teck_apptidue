}
var fromLat,fromLng;
function getPosition(position)
{
  fromLat = position.coords.latitude;
  fromLng = position.coords.longitude;
}
function calcRoute() {
   var lat = "<?php echo $resLat; ?>";
  var lng = "<?php echo $resLon; ?>";
  var start = $(".fromLocation").val();
  if(start=="My Location")
  {
    if (navigator.geolocation) {
        navigator.geolocation.getCurrentPosition(getPosition);
    } else {
        alert("Geolocation is not supported by this browser.");
    }
  }
  var travelMode = $(".travelMode").val();
var mode ;

switch(travelMode)  
{
  case "DRIVING":
  mode =  google.maps.TravelMode.DRIVING;
  break;
  case "BICYCLING":
  mode =  google.maps.TravelMode.BICYCLING;
  case "TRANSIT" :
  break;
  mode =  google.maps.TravelMode.TRANSIT;
  break;
  case "WALKING" :
  mode =  google.maps.TravelMode.WALKING;
  break;
}
  var end = new google.maps.LatLng(lat,lng);
  if(start=="My Location")
  {
    start = new google.maps.LatLng(fromLat,fromLng);
  }
  var request = {
    origin:start,
    destination:end,
    travelMode: mode
  };
  directionsService.route(request, function(result, status) {
    if (status == google.maps.DirectionsStatus.OK) {
      directionsDisplay.setDirections(result);
        $("#map-simple").parent().attr("class","col-md-8");
        marker.setMap(null);
        directionsDisplay.setMap(map);
        $("#right-panel").fadeIn();
        directionsDisplay.setPanel(document.getElementById('right-panel'));
    }
  });
}
